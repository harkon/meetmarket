<div id="wrapper">
  {{> navigation}}
  <div class="container-fluid">
    <!-- Page Heading -->
    <div class="row">
      <div class="col-lg-12">
        <h1 class="page-header">{{pageTitle}} <small>{{pageDesc}}</small></h1>
        <ol class="breadcrumb">
          <li class="active">
            <i class="fa {{pageIcon}}"></i> {{pageTitle}}
          </li>
        </ol>
      </div>
    </div>
    <!-- /.row -->
    <div class="row">
      <div class="list-group">
        {{#each stores}}
        <a href="/stores/{{_id}}" class="list-group-item">{{name}}</a>
        {{/each}}
      </div>
    </div>
  </div>
  <!-- /.container -->
</div>
<!-- /#wrapper -->