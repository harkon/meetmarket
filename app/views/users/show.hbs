<script type="text/javascript">
(function(){
function clickHandler(){
// console.log("clicked", window.location);
window.location.href = window.location.origin + '/stores/' + this.getAttribute("data-shop");
}
var table = document.getElementById('stores');
var rows = table.getElementsByTagName('tr');
for(i=0; i < rows.length; i++) {
rows[i].addEventListener('click', clickHandler, false);
}
})();
</script> --}}